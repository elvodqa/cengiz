{
	"authors": [
		"Emir Cengiz"
	],
	"buildTypes": {
		"dg_debug": {
			"buildOptions": [
				"debugMode",
				"debugInfoC",
				"alwaysStackFrame",
				"unittests",
				"warnings"
			]
		},
		"dg_release": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize",
				"warnings"
			]
		}
	},
	"configurations": [
		{
			"dflags": [
				"-Dd../docs",
				"-debug"
			],
			"importPaths": [
				"../"
			],
			"name": "main",
			"sourceFiles": [
				"main.d"
			],
			"sourcePaths": [
				"../"
			],
			"targetName": "cengiz-example",
			"targetType": "executable"
		}
	],
	"copyright": "Copyright Â© 2023, Emir Cengiz",
	"dependencies": {
		"bindbc-imgui": "~>0.7.0",
		"bindbc-opengl": "~>0.13.0",
		"bindbc-sdl": "~>1.4.1"
	},
	"description": "A minimal D application.",
	"lflags": [
		"-L../../../lib"
	],
	"libs": [
		"SDL2"
	],
	"license": "proprietary",
	"name": "cengiz",
	"versions": [
		"GL_41",
		"GL_ARB",
		"GL_AllowDeprecated",
		"SDL_2022",
		"USE_SDL2"
	]
}